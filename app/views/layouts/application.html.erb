<!DOCTYPE html>
<html>
  <head>
    <title><%= page_title %></title>
    <!--[if lt IE 9]>
    <%= javascript_include_tag "html5shiv.js" %>
    <![endif]-->
    <%= stylesheet_link_tag 'yui/reset',
      'jquery.svg',
      'ui-lightness/jquery-ui-1.8.5.custom',
      'application' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="body_wrapper">
      <div id="main_wrapper">
        <%= render :partial => 'layouts/header' %>
        <div id="center_wrapper">
          <%= yield %>
        </div>
      </div>
      <%= render :partial => 'layouts/footer' %>
    </div>
    <%= javascript_include_tag(
      'lib/jquery-1.4.2.min.js',
      'lib/jquery-ui-1.8.5.custom.min.js',
      'lib/jquery.svg.js',
      'lib/jquery.svganim.js',
      'lib/jquery.hotkeys.js',
      'district_map.js',
      'application.js',
      'data/states.js',
      'data/districts.js',
      :cache => true) %>
    <%= content_for :javascript %>
  </body>
</html>

